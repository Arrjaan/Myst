Options +FollowSymlinks
RewriteEngine on

RewriteRule ^admin/contact index.php?page=contact
RewriteRule ^admin/home index.php?a_page=home
RewriteRule ^admin/berichten(.*) index.php?a_page=berichten$1
RewriteRule ^admin/beheerders index.php?a_page=beheerders
RewriteRule ^admin/verwijder_beheerder(.*) index.php?a_page=verwijder_beheerder$1
RewriteRule ^admin/ander_wachtwoord(.*) index.php?a_page=ander_wachtwoord$1
RewriteRule ^admin/nieuwe_beheerder index.php?a_page=nieuwe_beheerder
RewriteRule ^admin/log index.php?a_page=log
RewriteRule ^admin/leeg_log(.*) index.php?a_page=leeg_log$1
RewriteRule ^admin/server index.php?a_page=server
RewriteRule ^admin/website index.php?a_page=website
RewriteRule ^admin/unset index.php?a_page=admin_off
RewriteRule ^admin/nieuw_bericht(.*) index.php?a_page=nieuw_bericht$1
RewriteRule ^admin/verwijder_bericht(.*) index.php?a_page=verwijder_bericht$1
RewriteRule ^admin/bewerk_bericht(.*) index.php?a_page=bewerk_bericht$1
RewriteRule ^admin/login.css 9C75.php
RewriteRule ^.htaccess index.php

<Files .htaccess>
	order deny,allow
	deny from all
</Files> 